import java.util.Date;

public class School 
{
	public static int sum(int n, int m)
	{
		return n+m;
	}
	public static void main(String[] args)
		{
			int i = 20;
			int s;
			char a;

			s = sum(i, 10);
			a = '?';
			System.out.println(a);
			System.out.println("School");
			System.out.println(s);
			
			Date today=new Date();
			System.out.println(today);
		}
	
}